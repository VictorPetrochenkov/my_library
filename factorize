# Функция факторизации (разложение числа на простые сомножители) числа
def factorize(n):
    fac_num = [1]    
    while n%2 == 0:
        fac_num.append(2)
        n //=2
    for i in range(1,round((n**(1/2)-1)/2+1)):        
        if n%(2*i+1) == 0:
            n //=(2*i+1)
            fac_num.append(2*i+1)
            i = i - 1
    if n !=1:
        fac_num.append(n)
    return fac_num
